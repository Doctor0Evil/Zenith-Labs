        mkdir -p ~/.aln/dev_resources/cheatsheets
        mkdir -p ~/.aln/dev_resources/configs
        mkdir -p ~/.aln/dev_resources/logs

        # Initialize configuration
        CONFIG = {
            "version": version,
            "compliance": compliance,
            "platform": platform,
            "runtime": "REAL",
            "self_evolution": true
        }
        echo -e "${CONFIG | json}" > ~/.aln/dev_resources/config.json
    }
}

